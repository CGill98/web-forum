yles.page}>\r\n            <Header />\r\n            <div className={globalstyles.content}>\r\n                <form className={styles.loginform} onSubmit={e=>handleSubmit(e)}>\r\n                    {state.form_error && <div className={styles.formerror}>{state.form_error}</div>}\r\n\r\n                    <div>Username or Email</div>\r\n                    <input type='text' placeholder=\"UsernameEmail\" value={state.emailusername} onChange={(e)=>dispatch({type: 'emailusername', payload: e.target.value})}></input>\r\n                    <div>Password</div>\r\n                    <input type='password' value={state.password} onChange={(e)=>dispatch({type: 'password', payload: e.target.value})}></input>\r\n                    <input type='submit' value='Login' className={styles.loginbtn}></input>\r\n                    <div>\r\n                        Forgot your details?\r\n                    </div>\r\n                </form>\r\n            </div>\r\n        </div>\r\n    )\r\n}\r\n\r\nexport default Login\r\n","C:\\Users\\Cooper\\VSCodeWorkSpace\\web-forum\\src\\pages\\Register.js",[],"C:\\Users\\Cooper\\VSCodeWorkSpace\\web-forum\\src\\api\\register.js",[],"C:\\Users\\Cooper\\VSCodeWorkSpace\\web-forum\\src\\reducers\\index.js",[],"C:\\Users\\Cooper\\VSCodeWorkSpace\\web-forum\\src\\reducers\\user.js",[],"C:\\Users\\Cooper\\VSCodeWorkSpace\\web-forum\\src\\api\\login.js",[],"C:\\Users\\Cooper\\VSCodeWorkSpace\\web-forum\\src\\api\\post.js",[],"C:\\Users\\Cooper\\VSCodeWorkSpace\\web-forum\\src\\api\\getPosts.js",[],{"ruleId":"108","replacedBy":"109"},{"ruleId":"110","replacedBy":"111"},{"ruleId":"112","severity":1,"message":"113","line":15,"column":3,"nodeType":"114","messageId":"115","endLine":15,"endColumn":7},{"ruleId":"112","severity":1,"message":"116","line":1,"column":16,"nodeType":"114","messageId":"115","endLine":1,"endColumn":25},{"ruleId":"112","severity":1,"message":"117","line":3,"column":22,"nodeType":"114","messageId":"115","endLine":3,"endColumn":2